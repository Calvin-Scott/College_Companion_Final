<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teamduke.collegecompanion" >

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-feature android:name="android.hardware.camera" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/lincoln_college_logo"
        android:label="@string/app_name"
        android:theme="@style/CollegeMaterialTheme" >
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".MoodleActivity"
            android:label="@string/title_activity_moodle" >
        </activity>
        <activity
            android:name=".WebmailActivity"
            android:label="@string/title_activity_webmail"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".LibraryActivity"
            android:label="@string/title_activity_library"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".BookActivity"
            android:label="@string/title_activity_book"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".BuildingActivity"
            android:label="@string/title_activity_building"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".RegistrationActivity"
            android:label="@string/title_activity_registration"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".StudentCardActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_student_card"
            android:screenOrientation="landscape"
            android:theme="@style/FullscreenTheme" >
        </activity>
        <activity
            android:name=".abbyActivity"
            android:label="@string/title_activity_abby" >
        </activity>
        <activity
            android:name=".BishopsActivity"
            android:label="@string/title_activity_bishops" >
        </activity>
        <activity
            android:name=".CatherdralActivity"
            android:label="@string/title_activity_catherdral" >
        </activity>
        <activity
            android:name=".DeansActivity"
            android:label="@string/title_activity_deans" >
        </activity>
        <activity
            android:name=".gibneyActivity"
            android:label="@string/title_activity_gibney" >
        </activity>
        <activity
            android:name=".KnightsActivity"
            android:label="@string/title_activity_knights" >
        </activity>
        <activity
            android:name=".MonksActivity"
            android:label="@string/title_activity_monks" >
        </activity>
        <activity
            android:name=".SessionsActivity"
            android:label="@string/title_activity_sessions" >
        </activity>
        <activity
            android:name=".TempleActivity"
            android:label="@string/title_activity_temple" >
        </activity>
    </application>

</manifest>
